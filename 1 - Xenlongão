using System;
{
    class DIO
    {
        static void Main(string[] args)
        {
          int N = Convert.ToInt32(Console.ReadLine());
          while(N-- > 0)
           {
                int c = Convert.ToInt32(Console.ReadLine());
                int r = (int)Math.Sqrt(c);
                int s = c - r;
           }
           Console.WriteLine(s);
        }
    }
}
